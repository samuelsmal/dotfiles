ctrl_interface=/var/run/wpa_supplicant
network={
  ssid="epfl"
  key_mgmt=WPA-EAP
  proto=WPA2
  eap=PEAP
  identity="samuel@epfl.ch"
  password="my_password"
  anonymous_identity="anonymous@epfl.ch"
  phase2="auth=MSCHAPV2"
